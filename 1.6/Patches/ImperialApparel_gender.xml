<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Imperial Attires</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="CAT_Apparel_ImperialDancerTop"]/apparel</xpath>
                    <value>
                        <gender>Female</gender>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="CAT_Apparel_ImperialDancerBottom"]/apparel</xpath>
                    <value>
                        <gender>Female</gender>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="CAT_Apparel_LuxuryLingerie"]/apparel</xpath>
                    <value>
                        <gender>Female</gender>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="CAT_Apparel_LuxuryPanty"]/apparel</xpath>
                    <value>
                        <gender>Female</gender>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>

</Patch>